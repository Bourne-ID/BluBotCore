<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>

    <!--<RuntimeIdentifier>ubuntu.16.04-x64</RuntimeIdentifier>-->
    
    <RuntimeIdentifier>win10-x64</RuntimeIdentifier>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <LangVersion>7.1</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Addons.EmojiTools" Version="2.0.2" />
    <PackageReference Include="Discord.Addons.Interactive" Version="1.0.1" />
    <PackageReference Include="Discord.Addons.Preconditions" Version="1.2.0-alpha3" />
    <PackageReference Include="Discord.Net" Version="2.0.1-dev-01055" />
    <PackageReference Include="Discord.Net.Commands" Version="2.0.1-dev-01055" />
    <PackageReference Include="Discord.Net.Core" Version="2.0.1-dev-01055" />
    <PackageReference Include="Discord.Net.Providers.WS4Net" Version="2.0.1-dev-01055" />
    <PackageReference Include="Discord.Net.Webhook" Version="2.0.1-dev-01055" />
    <PackageReference Include="Discord.Net.WebSocket" Version="2.0.1-dev-01055" />
    <PackageReference Include="SteamStoreQuery" Version="1.0.4" />
    <PackageReference Include="StrawPollNET" Version="1.0.2" />
    <PackageReference Include="System.Security.Cryptography.Algorithms" Version="4.3.1" />
    <PackageReference Include="TweetinviAPI" Version="4.0.0" />
    <PackageReference Include="TwitchLib" Version="3.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="setup.txt">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
